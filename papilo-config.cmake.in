if(NOT TARGET papilo)
  include(${CMAKE_CURRENT_LIST_DIR}/papilo-targets.cmake)
endif()
set(PAPILO_IMPORTED_TARGETS papilo)
set(PAPILO_FOUND 1)
set(TBB_DIR @TBB_CONFIG_LOCATION@)
find_package(TBB QUIET COMPONENTS tbb)
if(NOT TBB_FOUND)
   set(PAPILO_FOUND 0)
endif()

if(@PAPILO_HAVE_FLOAT128@ AND PAPILO_FOUND)
   set(CMAKE_MODULE_PATH ${CMAKE_MODULE_PATH} @QUADMATH_MODULE_PATH@)
   find_package(Quadmath)
   if(NOT Quadmath_FOUND)
      set(PAPILO_FOUND 0)
   endif()
endif()

if(@PAPILO_HAVE_LUSOL@ AND PAPILO_FOUND)
   enable_language(Fortran)
endif()
